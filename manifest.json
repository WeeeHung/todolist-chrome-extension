{
    "name": "toDoList",
    "description": "to do list to sort work",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "notifications",
        "storage"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "action": {
        "default_popup": "todolist.html",
        "default_title": "To Do List",
        "default_state": "enabled"
    }
}